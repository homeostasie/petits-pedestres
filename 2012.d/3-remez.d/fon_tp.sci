//Liste des fonctions utilisées pour le TP

function [fon_sqrt]= F_fon_sqrt(x)
  fon_sqrt=sqrt(x)
endfunction

function [fon_abs]= F_fon_abs(x)
  fon_abs=abs(x)
endfunction

function [fon_xpuisn]= F_xpuisn(x,n)
  fon_xpuisn=x**n
endfunction

function [fon_car]= F_fon_car(x)
  fon_car=1-3*x+x^2
endfunction


